<?xml version="1.0" encoding="utf-8"?>
<preference-headers
        xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Lockscreen Settings -->
    <header
        android:id="@+id/lockscreen_settings"
        android:fragment="com.evervolv.toolbox.activities.Lockscreen"
        android:title="@string/pref_lockscreen_title_head"
        />
    <!-- Statusbar Settings -->
    <header
        android:id="@+id/statusbar_settings"
        android:fragment="com.evervolv.toolbox.activities.Statusbar"
        android:title="@string/pref_statusbar_title_head"
        />
    <!-- Interface Settings -->
    <header
        android:id="@+id/interface_settings"
        android:fragment="com.evervolv.toolbox.activities.Interface"
        android:title="@string/pref_interface_title_head"
        />
    <!-- Notification Toolbox Settings -->
    <header
        android:id="@+id/notification_toolbox_settings"
        android:fragment="com.evervolv.toolbox.activities.NotificationToolbox"
        android:title="@string/pref_notification_toolbox_title_head"
        />
    <!-- Performance Settings -->
    <header
        android:id="@+id/performance_settings"
        android:fragment="com.evervolv.toolbox.activities.Performance"
        android:title="@string/pref_performance_title_head"
        />
</preference-headers>
